          "name" : "xfos"
          }
        ],
      "definition" : {
        "objectType" : "Func",
        "title" : "mvAlongNormals",
        "ports" : [
          {
            "objectType" : "Port",
            "nodePortType" : "IO",
            "name" : "this",
            "execPortType" : "IO",
            "typeSpec" : "PolygonMesh"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "length",
            "execPortType" : "In",
            "typeSpec" : "Float32"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "center",
            "execPortType" : "In",
            "typeSpec" : "Vec3"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "distance",
            "execPortType" : "In",
            "typeSpec" : "Float32"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "In",
            "name" : "xfos",
            "execPortType" : "Out",
            "typeSpec" : "Xfo[]"
            }
          ],
        "extDeps" : {},
        "code" : "dfgEntry
{
  // init xfos.
  xfos.resize(0);

  // nothing to do?
  if (length == 0)
    return;
    
  // if distance is illegal then return early.
  if (distance <= 0)
    return;

  // go through all the meshes point positions. 
  for (UInt32 i=0;i<this.pointCount();i++)
  {
    // get i-th point position and normal.
    Vec3 p = this.getPointPosition(i);
    Vec3 n = this.getPointNormal(i);

    // calculate vector from center to p.
    Vec3 v = p - center;

    // calculate amount of \"pushing\".
    Float32 f = length *  (1.0 - v.length() / distance);
   
    // push amount greater than zero?
    if (f > 0)
    {
      // calculate and set new point position.
      p += f * n;
      this.setPointPosition(i, p);
      
      // add a Xfo to the output array xfos.
      Xfo xfo;
      xfo.ori.setFromDirectionAndUpvector(n, Vec3(0.111, 2.222, 0.111));
      xfo.tr = p;
      xfo.sc = 0.5 * f;
      xfos.push(xfo);
    }
 }
}
"
        }
      },
